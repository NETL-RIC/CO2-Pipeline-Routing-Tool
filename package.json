{
  "name": "CO2PipelineRouteTool",
  "version": "0.1.0",
  "private": true,
  "author": "NETL",
  "description": "CO2 Pipeline Electron App",
  "homepage": "./",
  "proxy": "http://127.0.0.1:5000",
  "main": "./public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electronmon": "^2.0.3",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-esri-leaflet": "^2.0.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.0.1",
    "react-widgets": "^5.8.4",
    "shpjs": "^4.0.4",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-backend": "cd Flask && flask run --no-debugger",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "CO2Pipeline",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": [
      "dist/CO2PRT_Flask.exe"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis"
    }
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "electron": "^31.0.0",
    "electron-builder": "^24.6.3"
  }
}
